{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "suleman43/blog-website-frontend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 5173,
          "hostPort": 80
        }
      ]
    },
    {
      "name": "backend",
      "image": "suleman43/blog-website-backend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ]
    }
  ]
}
